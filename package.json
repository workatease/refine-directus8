{
  "name": "@workatease/refine-directus8",
  "description": "Refine Directus 8 REST data provider. This is a wrapper around the Directus SDK to use in refine.",
  "version": "0.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "git@github.com:workatease/refine-directus8.git",
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.json",
    "test": "tsdx test --passWithNoTests --runInBand",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "author": "GomezAtWork",
  "module": "dist/refine-directus.esm.js",
  "devDependencies": {
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@directus/sdk-js": "^6.3.0",
    "@pankod/refine-core": "^3.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}